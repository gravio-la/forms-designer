{
  "name": "@formswizard/designer-renderer",
  "version": "0.0.2",
  "description": "JsonForms renderers required by forms-designer.",
  "keywords": [
    "renderer", "material", "jsonforms", "form", "forms", "wizard",
    "json", "schema", "jsonschema", "uischema", "frontend",
    "generator", "input", "renderengine"
  ],
  "license": "MIT",
  "homepage": "https://github.com/FormsWizard",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormsWizard/forms-designer.git"
  },
  "contributors": [
    { "name": "Johannes LÃ¶tzsch" },
    { "name": "Jakob Steege" },
    { "name": "winzlieb", "email": "winzlieb@c3d2.de" }
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"**/*.ts*\"",
    "lint-fix": "eslint --fix \"**/*.ts*\"",
    "generate:component": "turbo gen react-component",
    "push": "npm publish --access public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@formswizard/types": "workspace:*",
    "@types/lodash-es": "catalog:",
    "eslint": "^7.32.0",
    "eslint-config-formsdesigner": "workspace:*",
    "@formswizard/designer-tsconfig": "workspace:*",
    "typescript": "^4.5.2",
    "react": ">=18",
    "@formswizard/tsup-config": "workspace:*"
  },
  "peerDependencies": {
    "@jsonforms/core": "catalog:",
    "@jsonforms/material-renderers": "catalog:",
    "@jsonforms/react": "catalog:",
    "@mui/icons-material": "catalog:",
    "@mui/material": "catalog:",
    "react": "^16.9.0 || ^17.0.0 || ^18",
    "react-dnd": "^16.0.1",
    "react-redux": "^9.2.0",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:"
  },
  "dependencies": {
    "@formswizard/react-hooks": "workspace:*",
    "@formswizard/state": "workspace:*",
    "@formswizard/utils": "workspace:*",
    "@formswizard/tool-context": "workspace:*",
    "classnames": "^2.3.2",
    "lodash-es": "catalog:"
  }
}

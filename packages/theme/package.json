{
  "name": "@formswizard/theme",
  "version": "0.0.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "type-check": "tsc",
    "build": "tsup index.tsx --format esm --dts",
    "dev": "tsup index.tsx --format esm --watch --dts",
    "lint-fix": "eslint --fix \"**/*.ts*\"",
    "push": "bun publish --access public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-formsdesigner": "workspace:*",
    "@formswizard/designer-tsconfig": "workspace:*",
    "typescript": "^4.5.2",
    "react": ">=18",
    "@formswizard/tsup-config": "workspace:*"
  },
  "peerDependencies": {
    "@mui/icons-material": "catalog:",
    "@mui/material": "catalog:",
    "react": "^16.9.0 || ^17.0.0 || ^18"
  }
}

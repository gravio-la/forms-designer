{
  "name": "@formswizard/utils",
  "version": "0.0.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --dts",
    "dev": "tsup index.ts --format esm,cjs --watch --dts",
    "lint": "eslint \"**/*.ts*\"",
    "lint-fix": "eslint --fix \"**/*.ts*\"",
    "push": "bun publish --access public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/lodash-es": "catalog:",
    "eslint": "^7.32.0",
    "eslint-config-formsdesigner": "workspace:*",
    "@formswizard/designer-tsconfig": "workspace:*",
    "typescript": "^4.5.2",
    "react": ">=18",
    "@formswizard/tsup-config": "workspace:*"
  },
  "peerDependencies": {
    "@jsonforms/core": "^3"
  },
  "dependencies": {
    "lodash-es": "catalog:",
    "@formswizard/types": "workspace:*"
  }
}
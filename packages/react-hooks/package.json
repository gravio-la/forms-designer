{
  "name": "@formswizard/react-hooks",
  "version": "0.0.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --dts --external react",
    "dev": "tsup index.ts --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"**/*.ts*\"",
    "lint-fix": "eslint --fix \"**/*.ts*\"",
    "push": "bun publish --access public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@formswizard/types": "workspace:*",
    "eslint": "^8.45.0",
    "eslint-config-formsdesigner": "workspace:*",
    "@formswizard/designer-tsconfig": "workspace:*",
    "typescript": "^5.1.6",
    "react": "catalog:",
    "@formswizard/tsup-config": "workspace:*"
  },
  "peerDependencies": {
    "@dnd-kit/core": "catalog:",
    "@jsonforms/core": "catalog:",
    "lodash-es": "catalog:",
    "react": "^16.9.0 || ^17.0.0 || ^18"
  },
  "dependencies": {
    "@formswizard/state": "workspace:*",
    "@formswizard/utils": "workspace:*",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:"
  }
}
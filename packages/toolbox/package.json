{
  "name": "@formswizard/toolbox",
  "version": "0.0.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"**/*.ts*\"",
    "lint-fix": "eslint --fix \"**/*.ts*\"",
    "generate:component": "turbo gen react-component",
    "push": "bun publish --access public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@formswizard/types": "workspace:*",
    "eslint": "^7.32.0",
    "eslint-config-formsdesigner": "workspace:*",
    "@formswizard/designer-tsconfig": "workspace:*",
    "typescript": "^4.5.2",
    "react": ">=18",
    "@formswizard/tsup-config": "workspace:*"
  },
  "peerDependencies": {
    "@dnd-kit/core": "catalog:",
    "@dnd-kit/utilities": "catalog:",
    "@jsonforms/core": "catalog:",
    "@mui/icons-material": "catalog:",
    "@mui/lab": "catalog:",
    "@mui/material": "catalog:",
    "react": "^16.9.0 || ^17.0.0 || ^18",
    "react-redux": "^9.2.0"
  },
  "dependencies": {
    "@formswizard/react-hooks": "workspace:*",
    "@formswizard/state": "workspace:*",
    "@formswizard/utils": "workspace:*",
    "@formswizard/tool-context": "workspace:*",
    "@formswizard/i18n": "workspace:*",
    "@mui/system": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:"
  }
}
